{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "mocks": [
        "src/compat/mockGuard"
      ],
      "mocks/*": [
        "src/compat/mockGuard"
      ],
      "__mocks__": [
        "src/compat/mockGuard"
      ],
      "__mocks__/*": [
        "src/compat/mockGuard"
      ]
    }
  },
  "include": [
    "src"
  ]
}